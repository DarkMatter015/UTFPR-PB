Algoritmo "VerificaNumero"

Var
   num: real


Inicio
      escreva("Informe um número: ")
      leia(num)
      
      se num = 10 entao
         escreva(num, " é igual a 10")
      senao
           se num > 10 entao
               escreva(num, " é maior que 10")
           senao
                escreva(num, " é menor que 10")
           fimse
      fimse


Fimalgoritmo